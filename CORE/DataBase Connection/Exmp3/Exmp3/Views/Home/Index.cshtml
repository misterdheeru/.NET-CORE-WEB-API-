@model List<Exmp3.Models.Employees>

@{
    ViewData["Title"] = "List of Employees";
}

<h1>List of Employees</h1>

<table class="table">
    <thead>
        <tr>
            <th>Employee ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Address</th>
            <th>Gender</th>
            <th>Salary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Gmail</td>
                <td>@employee.Address</td>
                <td>@employee.Gender</td>
                <td>@employee.Salary</td>
            </tr>
        }
    </tbody>
</table>
